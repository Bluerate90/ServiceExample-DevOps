# All these values come from GitHub Secrets
# Copy to .env locally and fill with YOUR values
# NEVER commit actual .env file

# MongoDB
MONGO_INITDB_ROOT_USERNAME=CHANGE_ME_IN_SECRETS
MONGO_INITDB_ROOT_PASSWORD=CHANGE_ME_IN_SECRETS
MONGO_INITDB_DATABASE=ServiceExampleDB
MONGODB_URI=CHANGE_ME_IN_SECRETS

# Redis (TLS=false for local development)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_TLS=false
REDIS_PASSWORD=CHANGE_ME_IN_SECRETS

# NATS
NATS_URL=nats://nats:4222
NATS_JETSTREAM_ENABLED=true

# Application
ASPNETCORE_ENVIRONMENT=Development
ASPNETCORE_URLS=http://+:5000
