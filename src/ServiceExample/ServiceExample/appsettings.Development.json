{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  
  "MongoDB": {
    "ConnectionString": "mongodb://admin:password123@localhost:27017/ServiceExampleDB?authSource=admin",
    "DatabaseName": "ServiceExampleDB",
    "Timeout": 10000
  },
  
  "Redis": {
    "Host": "localhost",
    "Port": 6379,
    "Password": "redis_password",
    "Database": 0,
    "ConnectTimeout": 5000,
    "SSL": false
  },
  
  "NATS": {
    "Url": "nats://nats_user:nats_password@localhost:4222",
    "ReconnectWait": 2000,
    "MaxReconnectAttempts": 10,
    "ClusterEnabled": true,
    "JetStreamEnabled": true,
    "JetStreamDomain": "myapp"
  }
}
